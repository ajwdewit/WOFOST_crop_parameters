RNFST    = 0.010    ! residual N fraction in stems [kg N kg-1 dry biomass]
RNFRT    = 0.010    ! residual N fraction in roots [kg N kg-1 dry biomass]
RPFLV    = 0.00187  ! residual P fraction in leaves [kg P kg-1 dry biomass]
RPFST    = 0.00093  ! residual P fraction in stems [kg P kg-1 dry biomass]
RPFRT    = 0.00093  ! residual P fraction in roots [kg P kg-1 dry biomass]
RKFLV    = 0.0173   ! residual K fraction in leaves [kg K kg-1 dry biomass]
RKFST    = 0.0087   ! residual K fraction in stems [kg K kg-1 dry biomass]
RKFRT    = 0.0087   ! residual K fraction in roots [kg K kg-1 dry biomass]
